<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>
<?import javafx.stage.Stage?>

<fx:root title="Help - User Guide" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/help_icon.png" />
  </icons>
  <scene>
    <Scene>
         <HBox alignment="CENTER" minHeight="750.0" minWidth="750.0" prefWidth="750.0">
            <children>
               <WebView fx:id="webView" minHeight="750.0" minWidth="750.0" prefHeight="750.0" prefWidth="662.0" HBox.hgrow="ALWAYS">
                  <HBox.margin>
                     <Insets />
                  </HBox.margin>
               </WebView>
            </children>
            <opaqueInsets>
               <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
            </opaqueInsets>
         </HBox>
    </Scene>
  </scene>
</fx:root>
